timezone: 'Asia/Tokyo'

_export:
  !include : 'conf/files.dig'
  !include : 'conf/mail.dig'

+load:
  _parallel: true

  for_each>:
    datum: ${files}
  _do:
    sh>: embulk run embulk/load_${datum}.yml.liquid
    _env:
      file_date: ${moment(session_time).format("YYYYMMDD")}
      td_apikey: SECRET_KEY
      td_fancl_db: DB_NAME
      td_table: ${datum}_test